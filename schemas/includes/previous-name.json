{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "name": "PreviousName",
  "description": "A previous name of a company",
  "type": "object",
  "properties": {
    "company_name": {
      "type": "string",
      "minLength": 1
    },
    "end_date": {
      "type": "string",
      "format": "date"
    },
    "start_date": {
      "type": "string",
      "format": "date"
    }
  },
  "required": [
    "company_name"
  ]
}
